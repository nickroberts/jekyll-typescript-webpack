{
  "name": "hhs.connectedtech.com",
  "version": "1.0.0",
  "description": "Hurley Health Services",
  "main": "src/ts/app.js",
  "scripts": {
    "build": "npm run clean && npm run build:webpack && npm run build:jekyll && npm run build:html-minifier",
    "build:webpack": "cross-env NODE_ENV=production webpack --env=prod --progress --profile --colors",
    "build:jekyll": "cross-env JEKYLL_ENV=production bundle exec jekyll build",
    "build:html-minifier": "node scripts/html-minifier.js",
    "clean": "rimraf ./_site ./assets",
    "start": "npm run clean && webpack --progress --profile --colors && concurrently --names \"Jekyll,Webpack\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:start:jekyll\" \"wait-on http://localhost:4000 && npm run start:webpack-dev-server\"",
    "start:jekyll": "bundle exec jekyll serve",
    "start:webpack-dev-server": "webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "foundation-sites": "^6.5.3",
    "jquery": "^3.3.1",
    "motion-ui": "^2.0.3",
    "what-input": "^5.1.3"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.29",
    "autoprefixer": "^9.4.7",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "html-minifier": "^3.5.21",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
