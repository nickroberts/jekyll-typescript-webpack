{
  "name": "jekyll-typescript-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --color --raw 'npm run jekyll:watch' 'npm run webpack-dev-server'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config webpack/webpack.prod.config.js",
    "webpack-dev-server": "webpack-dev-server --config webpack/webpack.dev.config.js",
    "jekyll:build": "bundle exec jekyll build",
    "jekyll:build:gh-pages": "bundle exec jekyll build --config _config.yml,_config.gh-pages.yml",
    "jekyll:build:prod": "bundle exec jekyll build --config _config.yml,_config.production.yml",
    "jekyll:watch": "bundle exec jekyll build --watch",
    "imagemin": "node ./scripts/imagemin",
    "htmlmin": "node ./scripts/htmlmin",
    "build": "node ./scripts/build",
    "build:gh-pages": "npm run jekyll:build:gh-pages && npm run webpack && npm run imagemin && npm run htmlmin",
    "build:prod": "npm run jekyll:build:prod && npm run webpack && npm run imagemin && npm run htmlmin",
    "deploy": "node ./scripts/deploy",
    "gh-pages": "node ./scripts/gh-pages"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/foundation-sites": "^6.1.28",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "foundation-sites": "^6.2.4",
    "gh-pages": "^0.11.0",
    "html-minifier": "^3.1.0",
    "image-webpack-loader": "^3.0.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.3",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9",
    "webpack-merge": "^0.15.0",
    "winston": "^2.2.0"
  }
}
